<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="net.suteren.medicomp"
	android:versionCode="11"
	android:versionName="0.0.6alpha" >

	<uses-sdk android:minSdkVersion="4" />

	<application
		android:icon="@drawable/medicomp"
		android:label="@string/app_name" >
		<activity
			android:name=".ui.activity.DashboardActivity"
			android:launchMode="singleTask" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name=".ui.activity.PersonListActivity"
			android:label="@string/app_name"
			android:launchMode="singleTask" >
			<intent-filter>
				<category android:name="net.suteren.medicomp.PERSON" />
			</intent-filter>
		</activity>
		<activity android:name=".ui.activity.PersonProfileActivity" >
		</activity>
		<activity android:name=".ui.activity.RecordListActivity" >
		</activity>
		<activity android:name=".ui.activity.RecordProfileActivity" >
		</activity>
	</application>

</manifest>